<h1>ユーザー一覧</h1>

<%= link_to '新規登録', new_user_path ,class: 'btn btn-primary' %>

<table class="table">
  <thead>
    <tr>
        <th><%= User.human_attribute_name :name %></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
        <tr>
            <th><%= user.name %></th>
        </tr>
    <% end %>
  </tbody>
</table>
